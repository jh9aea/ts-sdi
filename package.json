{
  "name": "ts-sdi",
  "version": "0.0.6",
  "description": "simple di container for ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "build": "yarn run build:cjs && yarn run build:esm && yarn run build:umd && yarn run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "yarn run build && yarn pack",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "di",
    "javascript",
    "typescript",
    "node",
    "dependency injection"
  ],
  "author": "aea",
  "license": "MIT",
  "homepage": "https://github.com/jh9aea/ts-sdi",
  "repository": {
    "type": "git",
    "url": "git@github.com:jh9aea/ts-sdi.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.170",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.16",
    "typescript": "^4.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
